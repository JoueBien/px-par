{
  "name": "px-par",
  "version": "0.3.0",
  "description": "A tool css for converting pixel measurements to % for grids.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "mocha ./src/**/*.js",
    "build": "tsc",
    "installNode": "nvm install 20.14.0",
    "swapNode": "nvm use 20.14.0",
    "installTypescript": "npm install -g typescript@4.5.5",
    "typecheck": "tsc --noEmit --skipLibCheck --project tsconfig.json"
  },
  "author": "Zach radloff",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.14.7",
    "mocha": "^10.4.0",
    "sass": "^1.77.6",
    "sass-true": "^8.0.0",
    "typescript": "^5.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JoueBien/px-par.git"
  },
  "keywords": [
    "css",
    "px",
    "%",
    "grid"
  ],
  "bugs": {
    "url": "https://github.com/JoueBien/px-par/issues"
  },
  "homepage": "https://github.com/JoueBien/px-par#readme"
}
